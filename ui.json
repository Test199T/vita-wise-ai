    "database_queries": {
      "analysis_queries": [
        {
          "name": "user_overview",
          "query": "SELECT COUNT(*) as total_users, COUNT(CASE WHEN is_active THEN 1 END) as active_users, AVG(EXTRACT(YEAR FROM AGE(date_of_birth))) as avg_age FROM users"
        },
        {
          "name": "health_goals_summary", 
          "query": "SELECT goal_type, COUNT(*) as total, COUNT(CASE WHEN status='completed' THEN 1 END) as completed FROM health_goals GROUP BY goal_type"
        },
        {
          "name": "exercise_patterns",
          "query": "SELECT AVG(duration_minutes) as avg_duration, AVG(calories_burned) as avg_calories, COUNT(DISTINCT user_id) as active_exercisers FROM exercise_log WHERE exercise_date >= NOW() - INTERVAL '30 days'"
        },
        {
          "name": "sleep_quality_analysis",
          "query": "SELECT sleep_quality, COUNT(*) as count, AVG(total_sleep_hours) as avg_hours FROM sleep_log WHERE sleep_date >= NOW() - INTERVAL '30 days' GROUP BY sleep_quality"
        },
        {
          "name": "ai_insights_performance",
          "query": "SELECT insight_type, COUNT(*) as generated, AVG(confidence_score) as avg_confidence, COUNT(CASE WHEN is_implemented THEN 1 END) as implemented FROM ai_insights GROUP BY insight_type"
        },
        {
          "name": "user_engagement",
          "query": "SELECT COUNT(DISTINCT user_id) as chatting_users, AVG(message_count) as avg_messages FROM (SELECT user_id, COUNT(*) as message_count FROM chat_messages WHERE timestamp >= NOW() - INTERVAL '30 days' GROUP BY user_id) subq"
        },
        {
          "name": "health_risks",
          "query": "SELECT COUNT(CASE WHEN blood_pressure_systolic > 140 THEN 1 END) as high_bp, COUNT(CASE WHEN blood_sugar_mg_dl > 200 THEN 1 END) as high_sugar, COUNT(CASE WHEN bmi > 30 THEN 1 END) as obese FROM health_metrics hm JOIN users u ON hm.user_id = u.id WHERE hm.metric_date = (SELECT MAX(metric_date) FROM health_metrics hm2 WHERE hm2.user_id = hm.user_id)"
        }
      ]
    },
